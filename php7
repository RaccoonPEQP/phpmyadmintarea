CREATE DATABASE registro_empleados;

USE registro_empleados;

CREATE TABLE EMPLEADOS(
    id INT NOT NULL AUTO_INCREMENT,
    nombre VARCHAR(20) NOT NULL,
    apellido VARCHAR(20) NOT NULL,
    cargo VARCHAR(10) NOT NULL,
    PRIMARY KEY(id)
);

CREATE TIEMPO_EMPLEADO(
    hora_entrada NOT NULL AUTO_INCREMENT PRIMARY KEY,
    hora_salida NOT NULL AUTO_INCREMENT PRIMARY KEY,
    EMPLEADOS_id INT NOT NULL,
    FOREIGN KEY (EMPLEADOS_id) REFERENCES EMPLEADOS(id) ON DELETE CASCADE
);