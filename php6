CREATE DATABASE APPS;

USE APPS;

CREATE TABLE LICENSIAS(
    id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    licensia VARCHAR(10) NOT NULL,
    tipo VARCHAR(10) NOT NULL,
    duracion_licensia VARCHAR(10) NOT NULL,
);

CREATE TABLE APLICACIONES(
    id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    nombre_aplicacion VARCHAR(15) NOT NULL,
    uso VARCHAR(10) NOT NULL,
    LICENSIAS_id INT NOT NULL,
    FOREIGN KEY (LICENSIAS_id) REFERENCES LICENSIAS(id) on DELETE CASCADE
);